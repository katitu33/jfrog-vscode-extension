{
    "rules": {
        "no-string-throw": true,
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "curly": true,
        "class-name": true,
        "semicolon": [true, "always"],
        "triple-equals": true,
        "typedef": [true, "parameter", "property-declaration", "variable-declaration", "member-variable-declaration"]
    },
    "defaultSeverity": "warning"
}
